{
  "name": "eslint-plugin-codegen",
  "version": "0.19.0-0",
  "engines": {
    "node": ">=12.0.0"
  },
  "packageManager": "pnpm@8.10.2",
  "keywords": [
    "typescript",
    "type-check",
    "assert",
    "types",
    "typings",
    "test",
    "testing"
  ],
  "homepage": "https://github.com/mmkal/eslint-plugin-codegen#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mmkal/eslint-plugin-codegen.git"
  },
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.md"
  ],
  "np": {
    "cleanup": false
  },
  "scripts": {
    "prepare": "pnpm build",
    "eslint": "eslint --ext '.ts,.js,.md'",
    "lint": "tsc && eslint .",
    "build": "tsc -p tsconfig.lib.json",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/generator": "~7.12.0",
    "@babel/parser": "^7.11.5",
    "@babel/traverse": "^7.11.5",
    "expect": "^29.7.0",
    "fp-ts": "^2.1.0",
    "glob": "^10.3.10",
    "io-ts": "^2.2.4",
    "io-ts-extra": "^0.11.6",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.15",
    "read-pkg-up": "^7.0.1",
    "string.prototype.matchall": "^4.0.2"
  },
  "devDependencies": {
    "@babel/types": "7.12.11",
    "@types/babel__generator": "7.6.2",
    "@types/babel__traverse": "7.11.0",
    "@types/dedent": "0.7.0",
    "@types/eslint": "7.2.6",
    "@types/glob": "7.1.3",
    "@types/jest": "29.5.8",
    "@types/js-yaml": "3.12.5",
    "@types/lodash": "4.14.201",
    "@types/minimatch": "3.0.3",
    "@types/node": "^20.0.0",
    "dedent": "0.7.0",
    "eslint": "8.54.0",
    "eslint-plugin-mmkal": "0.2.0",
    "expect-type2": "npm:expect-type@0.14.0",
    "jest": "29.7.0",
    "minimatch": "3.0.4",
    "np": "8.0.4",
    "prettier-2": "npm:prettier@^2",
    "strip-ansi": "6.0.1",
    "ts-jest": "29.1.1",
    "ts-morph": "16.0.0",
    "ts-node": "9.1.1",
    "type-fest": "^3.4.0",
    "typescript": "^5.2.2"
  }
}
