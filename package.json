{
  "name": "eslint-plugin-codegen",
  "version": "0.26.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@8.10.2",
  "keywords": [
    "typescript",
    "type-check",
    "assert",
    "types",
    "typings",
    "test",
    "testing"
  ],
  "homepage": "https://github.com/mmkal/eslint-plugin-codegen#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mmkal/eslint-plugin-codegen.git"
  },
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.md"
  ],
  "scripts": {
    "prepare": "pnpm build",
    "eslint": "eslint --max-warnings 0",
    "lint": "tsc && pnpm eslint .",
    "build": "tsc -p tsconfig.lib.json",
    "test": "pnpm build && vitest run",
    "e2e": "playwright test"
  },
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/generator": "~7.12.0",
    "@babel/parser": "^7.11.5",
    "@babel/traverse": "^7.11.5",
    "@pnpm/deps.graph-sequencer": "^1.0.0",
    "@types/dedent": "0.7.0",
    "@types/eslint": "^8.44.7",
    "@types/glob": "7.1.3",
    "@types/js-yaml": "3.12.5",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.0.0",
    "dedent": "^1.5.1",
    "eslint-plugin-markdown": "^3.0.1",
    "expect": "^29.7.0",
    "fp-ts": "^2.1.0",
    "glob": "^10.3.10",
    "io-ts": "^2.2.4",
    "io-ts-extra": "^0.11.6",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.15",
    "read-pkg-up": "^7.0.1"
  },
  "devDependencies": {
    "@babel/types": "7.12.11",
    "@playwright/test": "^1.40.0",
    "@types/babel__generator": "7.6.2",
    "@types/babel__traverse": "7.11.0",
    "@types/dedent": "0.7.0",
    "@types/glob": "7.1.3",
    "@types/js-yaml": "3.12.5",
    "@types/minimatch": "3.0.3",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.57.0",
    "eslint-plugin-mmkal": "^0.4.1",
    "minimatch": "3.0.4",
    "np": "^10.0.0",
    "strip-ansi": "6.0.1",
    "ts-morph": "16.0.0",
    "ts-node": "9.1.1",
    "type-fest": "^3.4.0",
    "typescript": "^5.2.2",
    "vitest": "^1.4.0"
  }
}
